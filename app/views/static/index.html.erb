<h1>Static#index</h1>
<p>Find me in app/views/static/index.html.erb</p>

<div id='calendar'></div>
<script>
  $('#calendar').fullCalendar({
    events: '/events.json',
    eventRender: function(event, element) {
        element.qtip({
            content: event.description
        });
    }
});
</script>